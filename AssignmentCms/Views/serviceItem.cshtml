@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage
@{
	Layout = "childPageLayout.cshtml";
    ViewBag.Title = Model?.Value("pageTitle");
}
<section class="spacing-top spacing-bottom">
    <div class="container service-details">
        <aside class="aside">
            <h5 class="h5">About us</h5>
            <img src="/static_media/service2.svg" alt="" />
            <p class="body">Dolor in reprehenderit in voluptate velit esse cillum dolore eu fugiat nulla pariatur. Excepteur sint occaecat cupidatat non </p>
        
            @await Html.PartialAsync("partials/static/helpFormSmall.cshtml")
        
        </aside>
        <div class="page-content">
            @Model.Value("pageContent")
        </div>
    </div>
    <div class="container">
     @await Html.PartialAsync("partials/static/helpFormBig.cshtml")
    </div>
</section>